<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/prof"
    tools:context=".SProfile">


    <TextView
        android:id="@+id/id_contact"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="361dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="161dp"
        android:text="Contact         :"
        android:textColor="#000000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/id_ssoname"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="262dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="260dp"
        android:text="SSO Name    :"
        android:textColor="#000000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/id_isono"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="463dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="59dp"
        android:text="ISO No.        :"
        android:textColor="#000000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/id_account"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="511dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="11dp"
        android:text="Account No. :"
        android:textColor="#000000"
        android:textSize="17sp" />


    <ImageView
        android:id="@+id/id_picture"
        android:layout_width="82dp"
        android:layout_height="74dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="235dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="43dp"
        android:src="@drawable/grp3" />

    <TextView
        android:id="@+id/id_email"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="406dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="116dp"
        android:text="Email ID       :"
        android:textColor="#000000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/id_address"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="210dp"
        android:text="Address        :"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/id_donate"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/id_picture"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="220dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="301dp"
        android:text="DONATE" />


</RelativeLayout>